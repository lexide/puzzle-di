{
  "name": "lexide/puzzle-di",
  "type": "composer-plugin",
  "description": "Puzzle DI, auto configuration for dependency injection in modules",
  "license": "MIT",
  "homepage": "https://github.com/lexide/puzzle-di",
  "require": {
    "php": ">=5.5.0",
    "composer-plugin-api": "^1.0 || ^2.0"
  },
  "require-dev": {
    "composer/composer": "2.0.*@dev",
    "phpunit/phpunit": "^4.0.0",
    "mockery/mockery": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Lexide\\PuzzleDI\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lexide\\PuzzleDI\\Test\\": "test"
    }
  },
  "extra": {
    "class": "Lexide\\PuzzleDI\\Plugin\\PuzzlePlugin"
  }
}
